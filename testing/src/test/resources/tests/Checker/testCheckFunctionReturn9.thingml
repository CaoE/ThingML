import "datatypes.thingml" from stl

thing TestCheckFunctionReturn
@checker_should_fail "false"
{
	//should fail as third, etc returns are unreachable
    function foo(a : Integer) : Integer do
    	`if (blah) {`
    		return 0 //should be ignored, as it is before the last extern statement
    	`}`
    	return 0 //OK, as this is the first return, which is also the last statement in the block
    end

    statechart TestAbstractFunction init INIT {
    	state INIT {
    	}
    }
}
