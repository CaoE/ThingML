import "../../thingml.thingml"

thing TestRegion2 includes Test 
@test "t # (IAC|ICA|ACI|AIC|CAI|CIA)(FBD|FDB|BDF|BFD|DBF|DFB)"
@test "ttt # (IAC|ICA|ACI|AIC|CAI|CIA)(FBD|FDB|BDF|BFD|DBF|DFB)(IAC|ICA|ACI|AIC|CAI|CIA)(FBD|FDB|BDF|BFD|DBF|DFB)"


{
	statechart TestRegion init I {
		internal event Test?Done
		action TestDone()
	                                  
		state I {
			on entry TestOut('\'I\'' as Char)
			transition -> F
			event m : Test?In
			guard m.c == '\'t\''
		}
		
		state F {
			on entry TestOut('\'F\'' as Char)
			transition -> I
			event m : Test?In
			guard m.c == '\'t\''
		}
		
		region R init A {
		
			state A {
				on entry TestOut('\'A\'' as Char)
				transition -> B
				event m : Test?In
				guard m.c == '\'t\''
			}
		
			state B {
				on entry TestOut('\'B\'' as Char)
				transition -> A
				event m : Test?In
				guard m.c == '\'t\''
			}
		
			
		}	

		region S init C {
	
			state C {
				on entry TestOut('\'C\'' as Char)
				transition -> D
				event m : Test?In
				guard m.c == '\'t\''
			}

			state D {
				on entry TestOut('\'D\'' as Char)
				transition -> C
				event m : Test?In
				guard m.c == '\'t\''
			}
		}
	}
}
