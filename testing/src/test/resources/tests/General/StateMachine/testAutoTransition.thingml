import "../../thingml.thingml"

thing TestAutoTransition includes Test 
@test "ttt # IJJKKK"
{
	statechart TestAutoTransition init I {
		state I {
			on entry TestOut('\'I\'' as Char)
			
			transition -> J
			action TestOut('\'J\'' as Char)
			
			transition -> K
			event m : Test?In
			guard m.c == '\'t\''
		}
		state J {
			on entry TestOut('\'J\'' as Char)
			
			transition -> K
			event m : Test?In
			guard m.c == '\'t\''
		}
		state K {
			on entry TestOut('\'K\'' as Char)
			
			transition -> K
			event m : Test?In
			guard m.c == '\'t\''
			
			internal event Test?Done
			action TestDone()
		}
	}
}
