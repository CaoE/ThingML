import "../../thingml.thingml"

thing TestInternalTransition2 includes Test 
@test "i # EI"
@test "t # ET"
@test "titi # ETITI"
{
	statechart TestInternalTransition2 init I {	
		on entry TestOut('\'E\'' as Char)
		
		internal event Test?Done
		action TestDone()
			
		internal
		event m : Test?In
		guard m.c == '\'i\''
		action TestOut('\'I\'' as Char)
			
		internal
		event m : Test?In
		guard m.c == '\'t\''
		action TestOut('\'T\'' as Char)	
	
		state I {
			
		}
	}
}
